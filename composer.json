{
  "name": "goedemiddag/laravel-schedule-monitor",
  "description": "Monitoring for scheduled jobs",
  "version": "1.0.0",
  "keywords": [
    "laravel",
    "sentry",
    "cron",
    "schedule"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Roy de Vos Burchart",
      "email": "roy@goedemiddag.nl"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/console": "^9.48",
    "illuminate/http": "^9.48",
    "illuminate/support": "^9.48"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.13",
    "phpstan/phpstan": "^1.9",
    "rector/rector": "^0.14.8",
    "orchestra/testbench": "^7.17",
    "guzzlehttp/guzzle": "^7.5"
  },
  "suggests": {
    "sentry/sentry-laravel": "^3.1"
  },
  "autoload": {
    "psr-4": {
      "Goedemiddag\\ScheduleMonitor\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Goedemiddag\\ScheduleMonitor\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Goedemiddag\\ScheduleMonitor\\ServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
